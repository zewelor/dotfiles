{
    "editor.accessibilitySupport": "off",
    "explorer.confirmDelete": false,
    "editor.tabSize": 2,
    "window.zoomLevel": 1,
    "vim.neovimPath": "/usr/bin/nvim",
    "workbench.colorTheme": "Solarized Light",
    "terminal.integrated.fontFamily": "'MesloLGS NF'",
    "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["u"],
        "after": [],
        "commands": [
            {
                "command": "undo",
                "args": []
            }
        ]
    },
    {
        "before": ["<C-r>"],
        "after": [],
        "commands": [
            {
                "command": "redo",
                "args": []
            }
        ]
    }
],
"ansible.validation": false,
"editor.renderControlCharacters": false,
"explorer.confirmDragAndDrop": false,
"redhat.telemetry.enabled": false,
"http.proxySupport": "off",
"editor.fontSize": 17,
"editor.fontFamily": "\"MesloLGS NF\"",
"ruby.codeCompletion": "rcodetools", //run non-lint commands with bundle exec
"update.mode": "none",
"editor.renderWhitespace": "none",
"editor.guides.indentation": false,
"ruby.intellisense": "rubyLocate",
"editor.minimap.enabled": false,
"sqltools.connections": [],
"telemetry.telemetryLevel": "off",
"files.insertFinalNewline": true,
"remote.containers.dockerComposePath": "docker compose",
"vim.easymotion": true,
"settingsSync.ignoredSettings": [
    "editor.fontSize"
],
"editor.inlineSuggest.enabled": true,
"solargraph.useBundler": true,
"solargraph.bundlerPath": "/home/omen/.asdf/shims/bundle",
"git.ignoreMissingGitWarning": true,
"[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
},
"git.enableSmartCommit": true,
"vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled"
},
"checkForMinikubeUpgrade": false,
"kubeseal.executablePath": "/home/omen/.local/bin/kubeseal",
"github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
},
"gitlens.currentLine.enabled": false,
"cSpell.userWords": [
    "stakater"
],
"cSpell.enableFiletypes": [
    "!yaml"
],
"gitlens.codeLens.authors.enabled": false,
"gitlens.codeLens.recentChange.enabled": false,
"git.confirmSync": false,
}