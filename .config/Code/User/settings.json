{
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[ruby]": {
        "editor.defaultFormatter": "Shopify.ruby-lsp"
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.defaultColorDecorators": "never",
        "editor.quickSuggestions": {
            "strings": true,
            "other": true,
            "comments": false
        }
    },
    "chat.agent.enabled": true,
    "chat.editing.confirmEditRequestRetry": false,
    "chat.mcp.gallery.enabled": true,
    "chat.tools.terminal.autoApprove": {
        "clear": true,
        "nvim --headless": true,
        "rg": true,
        "git clone": true,
        "/bin/ls": true,
        "/^mkdir -p temp && helm template victoria-metrics-k8s-stack charts/system-monitoring/victoria-metrics-k8s-stack > temp/after\\.yaml$/": {
            "approve": true,
            "matchCommandLine": true
        },
        "diff": true,
        "docker compose": true,
        "awk": true,
        "rsvg-convert": true,
        "tee": true,
        "uniq": true,
        "time": true,
        "git checkout": true
    },
    "dev.containers.defaultExtensions": [
        "GitHub.copilot"
    ],
    "dev.containers.dockerComposePath": "docker compose",
    "diffEditor.experimental.showMoves": true,
    "docker.extension.enableComposeLanguageServer": false,
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "'JetBrainsMonoNL Nerd Font Mono'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.guides.indentation": false,
    "editor.inlineSuggest.enabled": true,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "trailing",
    "editor.tabSize": 2,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        ".bash_history": "plaintext"
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.ignoreMissingGitWarning": true,
    "git.ignoreRebaseWarning": true,
    "git.openRepositoryInParentFolders": "never",
    "github.copilot.advanced": {},
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": true
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.selectedCompletionModel": "",
    "http.proxySupport": "off",
    "redhat.telemetry.enabled": false,
    "remote.autoForwardPorts": false,
    "remote.autoForwardPortsSource": "hybrid",
    "rubyLsp.enabledFeatures": {
        "codeActions": true,
        "diagnostics": true,
        "documentHighlights": true,
        "documentLink": true,
        "documentSymbols": true,
        "foldingRanges": true,
        "formatting": true,
        "hover": true,
        "inlayHint": true,
        "onTypeFormatting": true,
        "selectionRanges": true,
        "semanticHighlighting": true,
        "completion": true
    },
    "rubyLsp.featureFlags": {
        "all": true
    },
    "rubyLsp.rubyExecutablePath": "/home/omen/.local/share/mise/shims/ruby",
    "rubyLsp.rubyVersionManager": {
        "identifier": "auto"
    },
    "settingsSync.ignoredSettings": [
        "editor.fontSize"
    ],
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.suggest.enabled": true,
    "update.mode": "none",
    "vim.easymotion": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo",
                    "args": []
                }
            ]
        }
    ],
    "window.zoomLevel": 1,
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.enableImages": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                // Make comments not italic by default
                "scope": "comment",
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[*Light*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#000"
                    },
                }
            ]
        },
        "[*Dark*]": {
            "textMateRules": [
                {
                    "scope": "ref.matchtext",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        }
    },
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "disabled",
        "vs-kubernetes.knownKubeconfigs": [
            "/home/omen/.kube/config"
        ],
        "vs-kubernetes.kubeconfig": "/home/omen/.kube/config"
    },
    "workbench.colorTheme": "Solarized Light",
    "vim.smartRelativeLine": true,
    "remote.SSH.enableDynamicForwarding": false,
    "chat.tools.terminal.terminalProfile.linux": {
        "path": "/usr/bin/zsh",
        "env": {
            "ZSHRC_SKIP_CUSTOMIZATIONS": "1"
        }
    },
    "claudeCode.selectedModel": "default"
}
